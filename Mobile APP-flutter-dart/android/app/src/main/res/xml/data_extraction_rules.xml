<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Inclure toutes les préférences partagées -->
        <include domain="sharedpref" path="."/>
        <!-- Exclure les préférences qui contiennent des données sensibles -->
        <exclude domain="sharedpref" path="sensitive_data.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- Inclure toutes les préférences partagées -->
        <include domain="sharedpref" path="."/>
        <!-- Exclure les préférences qui contiennent des données sensibles -->
        <exclude domain="sharedpref" path="sensitive_data.xml"/>
    </device-transfer>
</data-extraction-rules> 